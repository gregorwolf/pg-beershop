{
  "welcomeFile": "/webapp/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/user-api(.*)",
      "target": "$1",
      "service": "sap-approuter-userapi"
    },
    {
      "source": "/beershop-admin/(.*)$",
      "target": "/beershop-admin/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/beershop/(.*)$",
      "target": "/beershop/$1",
      "authenticationType": "xsuaa",
      "destination": "srv_api",
      "csrfProtection": true
    },
    {
      "source": "/kyma/(.*)$",
      "target": "/$1",
      "authenticationType": "xsuaa",
      "destination": "KYMA"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "localDir": "./"
    }
  ]
}
